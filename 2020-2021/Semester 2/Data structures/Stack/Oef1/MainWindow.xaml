<Window x:Class="Oef1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oef1"
        mc:Ignorable="d"
        Title="MainWindow" Height="265" Width="439">
    <Grid>
        <TextBox x:Name="tbInput" HorizontalAlignment="Left" Height="52" Margin="10,10,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="184"/>
        <TextBox x:Name="tbOutput" HorizontalAlignment="Left" Height="172" Margin="10,67,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="184"/>
        <Button x:Name="btnPush" Content="Push" HorizontalAlignment="Left" Margin="241,10,0,0" VerticalAlignment="Top" Height="52" Width="188" Click="btnPush_Click"/>
        <Button x:Name="btnPop" Content="Pop" HorizontalAlignment="Left" Margin="241,67,0,0" VerticalAlignment="Top" Height="52" Width="188" Click="btnPop_Click"/>

    </Grid>
</Window>
